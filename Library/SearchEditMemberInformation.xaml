<Window x:Class="Library.SearchEditMemberInformation"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Library"
        mc:Ignorable="d"
        Title="SearchEditMemberInformation" Height="150" Width="490">
    <Grid Margin="0,0,-1.667,3.667">
        <Label Content="User Id :" HorizontalAlignment="Left" Margin="64,23,0,0" VerticalAlignment="Top" FontFamily="Ink Free" FontWeight="Bold" Height="24"/>
        <TextBox x:Name="UserIdTxtBx" HorizontalAlignment="Left" Margin="129,27,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="276" FontFamily="Ink Free" FontWeight="Bold"/>
        <Button x:Name="SearchBtn" Content="Search" HorizontalAlignment="Left" Margin="160,82,0,0" VerticalAlignment="Top" FontFamily="Ink Free" FontWeight="Bold" Width="55" Click="SearchBtn_Click"/>
        <Separator HorizontalAlignment="Left" Margin="60,62,0,0" VerticalAlignment="Top" Height="23" Width="375"/>
        <Button x:Name="BackBtn" Content="Back" HorizontalAlignment="Left" Margin="241,82,0,0" VerticalAlignment="Top" FontFamily="Ink Free" FontWeight="Bold" Width="55" Click="BackBtn_Click"/>

    </Grid>
</Window>
