<Window x:Name="MemberDashboard1" x:Class="Library.MemberDashboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Library"
        mc:Ignorable="d"
        Title="MemberDashboard" Height="440" Width="780" Loaded="MemberDashboard1_Loaded">
    <Grid Margin="0,0,17.333,10.667">
        <Label Content="Search by Book Title or Author name or Subject name  :" HorizontalAlignment="Left" Margin="133,77,0,0" VerticalAlignment="Top" Width="429" FontSize="16" FontFamily="Ink Free" FontWeight="Bold"/>
        <RichTextBox x:Name="SearchTxtBx" Margin="136,103,121,267" FontFamily="Ink Free" FontWeight="Bold">
            <FlowDocument>
                <Paragraph>
                    <Run Text=""/>
                </Paragraph>
            </FlowDocument>
        </RichTextBox>
        <Button x:Name="SearchBtn" Content="Search" HorizontalAlignment="Left" Margin="338,151,0,0" VerticalAlignment="Top" Width="86" Height="27" FontFamily="Ink Free" FontWeight="Bold" Click="SearchBtn_Click"/>
        <Button x:Name="BorrowedBookBtn" Content="Borrowed&#xA; Book" HorizontalAlignment="Left" Margin="237,237,0,0" VerticalAlignment="Top" Height="64" Width="99" Click="BorrowedBookBtn_Click" FontFamily="Ink Free" FontWeight="Bold"/>
        <Button x:Name="AccountDashboardBtn" Content="Account&#xA; Dashboard" HorizontalAlignment="Left" Margin="439,235,0,0" VerticalAlignment="Top" Height="64" Width="99" Click="AccountDashboardBtn_Click" FontFamily="Ink Free" FontWeight="Bold"/>
        <Button x:Name="LogoutBtn" Content="Logout" HorizontalAlignment="Left" Margin="336,351,0,0" VerticalAlignment="Top" Height="27" Width="86" Click="LogoutBtn_Click" FontFamily="Ink Free" FontWeight="Bold"/>
        <Separator HorizontalAlignment="Left" Margin="150,175,0,0" VerticalAlignment="Top" Height="48" Width="471"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="849,240,0,0" VerticalAlignment="Top" Width="0"/>
        <Separator HorizontalAlignment="Left" Margin="150,300,0,0" VerticalAlignment="Top" Height="48" Width="471"/>
        <TextBlock x:Name="FullNameTxtB" HorizontalAlignment="Left" Margin="122,13,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="525" Height="31" FontFamily="Ink Free" FontWeight="Bold" FontSize="14"/>
        <Separator HorizontalAlignment="Left" Margin="112,28,0,0" VerticalAlignment="Top" Height="48" Width="539"/>




    </Grid>
</Window>
