<Window x:Name="SignUp1" x:Class="Library.SignUp"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Library"
        mc:Ignorable="d"
        Title="SignUp" Height="322.667" Width="660" Loaded="SignUp1_Loaded_1">
    <Grid Margin="0,0,-2.667,-0.333">
        <Label Content="First Name :" HorizontalAlignment="Left" Margin="32,55,0,0" VerticalAlignment="Top" FontFamily="Ink Free" FontWeight="Bold" Height="24"/>
        <TextBox x:Name="FirstNameTxtBx" HorizontalAlignment="Left" Margin="109,59,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="135" FontFamily="Ink Free" FontWeight="Bold"/>
        <Label Content="LastName :" HorizontalAlignment="Left" Margin="261,55,0,0" VerticalAlignment="Top" FontFamily="Ink Free" FontWeight="Bold"/>
        <TextBox x:Name="LastNameTxtBx" HorizontalAlignment="Left" Margin="333,59,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="135" FontFamily="Ink Free" FontWeight="Bold"/>
        <Label Content="Email :" HorizontalAlignment="Left" Margin="63,86,0,0" VerticalAlignment="Top" FontFamily="Ink Free" FontWeight="Bold" Height="24"/>
        <TextBox x:Name="EmailTxtBx" HorizontalAlignment="Left" Margin="109,89,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="219" FontFamily="Ink Free" FontWeight="Bold"/>
        <Label Content="Username :" HorizontalAlignment="Left" Margin="343,86,0,0" VerticalAlignment="Top" FontFamily="Ink Free" FontWeight="Bold" Height="24"/>
        <TextBox x:Name="UsernameTxtBx" HorizontalAlignment="Left" Margin="413,89,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="212" FontFamily="Ink Free" FontWeight="Bold"/>
        <Label Content="Password :" HorizontalAlignment="Left" Margin="346,111,0,0" VerticalAlignment="Top" FontFamily="Ink Free" FontWeight="Bold" Height="24"/>
        <TextBox x:Name="PhoneNumberTxtBx" HorizontalAlignment="Left" Margin="109,115,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="219" FontFamily="Ink Free" FontWeight="Bold"/>
        <Label Content="Phone Number :" HorizontalAlignment="Left" Margin="17,111,0,0" VerticalAlignment="Top" FontFamily="Ink Free" FontWeight="Bold" Height="24"/>
        <Label Content="Birth Date :" HorizontalAlignment="Left" Margin="173,183,0,0" VerticalAlignment="Top" FontFamily="Ink Free" FontWeight="Bold" Height="24"/>
        <DatePicker x:Name="BirthDateDP" HorizontalAlignment="Left" Margin="250,183,0,0" VerticalAlignment="Top" Width="223" FontFamily="Ink Free" FontWeight="Bold" Height="25"/>
        <Separator HorizontalAlignment="Left" Margin="73,210,0,0" VerticalAlignment="Top" Height="24" Width="522" RenderTransformOrigin="0.5,0.5">
            <Separator.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.183"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Separator.RenderTransform>
        </Separator>
        <Button x:Name="SignUpBtn" Content="Sign Up" HorizontalAlignment="Left" Margin="278,242,0,0" VerticalAlignment="Top" Width="84" FontFamily="Ink Free" FontWeight="Bold" Height="23" Click="SignUpBtn_Click"/>
        <Label Content="Education :" HorizontalAlignment="Left" Margin="181,148,0,0" VerticalAlignment="Top" FontFamily="Ink Free" FontWeight="Bold" Height="24"/>
        <TextBox x:Name="EducationTxtBx" HorizontalAlignment="Left" Margin="253,153,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="222" FontFamily="Ink Free" FontWeight="Bold"/>
        <ComboBox x:Name="GenderCmBx" Height="21" Margin="536,57,32,0" VerticalAlignment="Top" Text="Gender" FontFamily="Ink Free" FontWeight="Bold" IsEditable="true"  IsReadOnly="true" 
            IsDropDownOpen="False" StaysOpenOnEdit="True" RenderTransformOrigin="0.555,0.551"  >
            <ComboBox.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.031"/>
                    <TranslateTransform/>
                </TransformGroup>
            </ComboBox.RenderTransform>
            <ComboBoxItem Content="Male" HorizontalAlignment="Left" Width="117.333333333333"/>
            <ComboBoxItem Content="Female" HorizontalAlignment="Left" Width="117.333333333333"/>
        </ComboBox>
        <PasswordBox x:Name="PasswordTxtBx" HorizontalAlignment="Left" Margin="413,117,0,0" VerticalAlignment="Top" Width="212" PasswordChar="*" FontFamily="Ink Free" FontWeight="Bold"/>
        <TextBlock x:Name="textblockTxB" HorizontalAlignment="Left" Margin="80,7,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="314" Height="27" FontFamily="Ink Free" FontWeight="Bold" FontSize="14"/>
        <Separator HorizontalAlignment="Left" Margin="79,25,0,0" VerticalAlignment="Top" Height="24" Width="522" RenderTransformOrigin="0.5,0.5">
            <Separator.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.183"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Separator.RenderTransform>
        </Separator>

    </Grid>
</Window>
